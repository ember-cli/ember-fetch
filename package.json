{
  "name": "ember-fetch",
  "version": "8.1.2",
  "description": "HTML5 Fetch polyfill (as an ember-addon)",
  "typings": "./index.d.ts",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/ember-cli/ember-fetch",
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "lerna-changelog",
    "release": "release-it",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test:ember": "ember test",
    "ember-compatibility-tests": "ember try:each",
    "test:node": "ember ts:precompile && mocha && ember ts:clean",
    "test:all": "yarn run test:node && ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.7.3",
    "broccoli-concat": "^4.2.5",
    "broccoli-debug": "^0.6.5",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-rollup": "^2.1.1",
    "broccoli-stew": "^3.0.0",
    "broccoli-templater": "^2.0.1",
    "calculate-cache-key-for-tree": "^2.0.0",
    "caniuse-api": "^3.0.0",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-typescript": "^4.1.0",
    "ember-cli-version-checker": "^5.1.2",
    "node-fetch": "^2.6.1",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@types/ember": "^3.16.5",
    "@types/ember-data": "^3.16.9",
    "@types/ember-qunit": "^3.4.13",
    "@types/ember__test-helpers": "^2.0.1",
    "@types/qunit": "^2.11.1",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-test-helper": "^2.0.0",
    "chai": "^4.3.4",
    "chai-fs": "^2.0.0",
    "ember-auto-import": "^1.12.0",
    "ember-cli": "~3.28.6",
    "ember-cli-addon-tests": "^0.11.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-htmlbars": "^5.7.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-data": "~3.20.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.8",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.7",
    "lerna-changelog": "^1.0.1",
    "loader.js": "^4.2.3",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "pretender": "^3.4.7",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^1.6.0",
    "release-it": "~14.2.2",
    "release-it-lerna-changelog": "^3.1.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "10.* || 12.* || 14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "fastbootDependencies": [
      "node-fetch",
      "abortcontroller-polyfill",
      "abortcontroller-polyfill/dist/cjs-ponyfill"
    ]
  },
  "volta": {
    "node": "10.24.1",
    "yarn": "1.22.10"
  }
}
